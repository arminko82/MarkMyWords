{
  "manifest_version": 2,
  "name": "MarkMyWords",
  "version": "0.8",
  "description": "Highlights all occurences of the current text selection.",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ]
}
